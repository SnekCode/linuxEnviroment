#git Aliases
alias gst='git status'
alias ga.='git add .'
alias gp='git push'
alias gcm='git checkout master && git pull'

gcamDes="gcam = git add all files then commit with message"
function gcam() {
ga. && git commit -m "$*" && echo Commit made with message:  "$*"
}


#
# Add functions above to this echo
#
alias aliasFNGit='echo Functions: && echo $gcamDes'
