#git Aliases
alias gst='git status'
alias ga.='git add .'
alias gp='git push'

# use gcm followed by the commit message with out quotes 
# to quickly commit messages
gcmDes="gcm  = git commit -m followed by the message provided"
function gcm() {
git commit -m "$*"
}

gcamDes="gcam = git add all files then commit with message"
function gcam() {
ga. && git commit -m "$*" && echo Commit made with message:  "$*"
}


#
# Add functions above to this echo
#
alias aliasFNGit='echo Functions: && echo $gcamDes && echo $gcmDes'
