#git Aliases
alias gst='git status'
alias ga.='git add .'

# use gcm followed by the commit message with out quotes 
# to quickly commit messages
function gcm() {
git commit -m "$@"
}

# use gcam to git add all files then commit with message
function gcam() {
ga. && git commit -m "$*" && echo Commit made with message:  "$*"
}
